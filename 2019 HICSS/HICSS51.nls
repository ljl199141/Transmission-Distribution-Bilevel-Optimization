\begin{thenomenclature} 

 \nomgroup{A}

\item [{\textbf{Indices}}]\begingroup \nomeqref {0}
		\nompageref{1}
 \item [{$t$}]\begingroup a time period (time interval from the beginning through the end of a period)\nomeqref {0}\nompageref{2}
 \item [{$g$}]\begingroup TS network generator index\nomeqref {0}\nompageref{2}
  \item [{$i$}]\begingroup DS network bus number index\nomeqref {0}\nompageref{2}
		
 \item [{\textbf{Parameters}}]\begingroup \nomeqref {0}
		\nompageref{1}
  \item [{$T$}]\begingroup length of the planning horizon\nomeqref {0}\nompageref{2}
  \item [{$G$}]\begingroup total number of generators in the TS\nomeqref {0}\nompageref{2}
  \item [{$\overline{R}_g$}]\begingroup TS generator upper ramp rate limit \nomeqref {0}
		\nompageref{2}
  \item [{$\underline{R}_g$}]\begingroup TS generator lower ramp rate limit \nomeqref {0}
		\nompageref{2}
  \item [{$\overline{P}_g$}]\begingroup TS generator generation upper bound \nomeqref {0}
		\nompageref{2}
  \item [{$\underline{P}_g$}]\begingroup TS generator generation lower bound \nomeqref {0}
		\nompageref{2}
  \item [{$Line$}]\begingroup TS line limit \nomeqref {0}
		\nompageref{2}
  \item [{$C_g$}]\begingroup TS generater g cost\nomeqref {0}
		\nompageref{1}		
  \item [{$C^r_{g}$}]\begingroup TS generater g reserve cost\nomeqref {0}
		\nompageref{1}		
  \item [{$C^c_{g}$}]\begingroup TS generator g commitment cost\nomeqref {0}
		\nompageref{1}
  \item [{$GSF$}]\begingroup TS generation shift factor matrix\nomeqref {0}
		\nompageref{1}
 \item [{$W^f$}]\begingroup forecasted wind power \nomeqref {0}
		\nompageref{2}		
  \item [{$W^{up}$}]\begingroup positive wind deviation from the forecast\nomeqref {0}
		\nompageref{1}
  \item [{$W^{dn}$}]\begingroup negative wind deviation from the forecast\nomeqref {0}
		\nompageref{1}
  \item [{$N_b$}]\begingroup number of buses in the TS\nomeqref {0}
		\nompageref{1}
  \item [{$N^d_b$}]\begingroup number of buses in the DS\nomeqref {0}
		\nompageref{1}
  \item [{$L_{t}$}]\begingroup TS load power vector in hour t\nomeqref {0}
