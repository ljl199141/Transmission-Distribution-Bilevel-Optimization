  \item [{$\overline{L}^d_{i}$}]\begingroup DS dispatchable load upper bound at bus i\nomeqref {0}
		\nompageref{1}	
  \item [{$ L^{ie}_{i}$}]\begingroup DS inelastic load in time t at bus i\nomeqref {0}
		\nompageref{1}
  \item [{$\overline{P}_{i}$}]\begingroup DS power flow to bus i upper bound\nomeqref {0}
		\nompageref{1}
  \item [{$\underline{P}_{i}$}]\begingroup DS power flow to bus i lower bound\nomeqref {0}
		\nompageref{1}
		
  \item [{$C^{d1}_{i}$}]\begingroup DS DG generation linear cost at bus i\nomeqref {0}
		\nompageref{1}
  \item [{$C^{d2}_{i}$}]\begingroup DS DG generation quadratic cost at bus i\nomeqref {0}
		\nompageref{1}
  \item [{$C^d$}]\begingroup DS utility for consuming dispatchable load\nomeqref {0}
		\nompageref{1}
  \item [{$C^{dr1}$}]\begingroup DS linear demand response cost\nomeqref {0}
		\nompageref{1}
  \item [{$C^{dr2}$}]\begingroup DS quadratic demand response cost \nomeqref {0}
		\nompageref{1}
  \item [{$\overline P^d_i$}]\begingroup DS DG generation upper bound at bus i\nomeqref {0}
		\nompageref{1}
  \item [{$\underline P^d_i$}]\begingroup DS DG generation lower bound at bus i\nomeqref {0}
		\nompageref{1}		
  \item [{$\underline S^(dr)$}]\begingroup DR scale factor\nomeqref {0}
		\nompageref{1}				
  \item [{\textbf{Variables}}]\begingroup \nomeqref {0}
		\nompageref{1}
  \item [{$r_{g,t}^{up}$}]\begingroup TS generator g upward reserve  in hour t\nomeqref {0}
		\nompageref{2}
 \item [{$r_{g,t}^{dn}$}]\begingroup TS generator g downward reserve  in hour t\nomeqref {0}
		\nompageref{2}
 \item [{$p^{dr}_{t}$}]\begingroup demand response price  in hour t\nomeqref {0}
		\nompageref{2}
  \item [{$p_{g,t}$}]\begingroup TS generator g generation  in hour t \nomeqref {0}
		\nompageref{2}
