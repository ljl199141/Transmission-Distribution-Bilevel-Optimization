		\nompageref{1}
  \item [{$P_{i,t}$}]\begingroup DS power flow to bus i in hour t\nomeqref {0}
		\nompageref{1}
  \item [{$dr_{t}^{up}$}]\begingroup DS  upward DR in hour t at bus i\nomeqref {0}
		\nompageref{1}
  \item [{$c_{t}^{im}$}]\begingroup DS energy import price in hour t\nomeqref {0}
		\nompageref{1}
  \item [{$p^{inj}_{t}$}]\begingroup bus power injection vector in hour t\nomeqref {0}
		\nompageref{2}
  \item [{$w_{g,t}$}]\begingroup TS generator commitment variable in hour t for generator g\nomeqref {0}
		\nompageref{1}
